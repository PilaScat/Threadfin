{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "ts/tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc build"
		},
		{
			"label": "Build debug version",
			"type": "shell",
			"windows":{
				"command": "go",
				"args": [
					"build",
					"-gcflags=all='-N -l'"
				]
			},
			"dependsOn" :[
				"tsc build"
			]
		}
	]
}